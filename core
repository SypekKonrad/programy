#response str 58

print('hi, name?')
def gowno1():
    name = input()
    if name == 'dupa':
        print('hi dupa')
        #wcisnac tu while z ifem?(podaj haslo itp.)
        while True:
            response = input()
            if response == 'hi':
                print('password, dupa?')
                while True:
                        password = input()
                        if password == 'ptaki lataja kluczem':
                            print('dada')
                        else:
                            print('haslo nieprawidlowe')
                            
                            return
    gowno1()
def gowno2():
    name = input()
    if name == 'dupa2':
        print(' hi dupa2')
    gowno2()
def gowno3():
    name = input('name?')
    if name == 'dupa1':
        print(' hi cwel36')
    gowno3()
def main():
    while True:
        gowno1()
    
        
       
        restart=input('ask again?')
        if restart == 'da':
            pass
        
        else:
            exit()
main()
    
#cel na nastepny dzien: zrobic dzialajacy restart z (przynajmniej) 1 funkcja
#jak zrobiæ ¿eby program nie glupial w przypadku kilku funkcji w main()

