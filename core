def gowno1():
    name = input()
    if name == 'dupa':
        print('hi dupa')
        #wcisnac tu while z ifem?(podaj haslo itp.)
        while True:
            response = input()
            if response == 'hi':
                
                print('password, dupa?')
                
                while True:
                        #gowno2()
                        password = input()
                        password == '123456'
                        for x in password:
                            print('''
                            *
                            **
                            ***''')
				
                        else:
                            print('haslo nieprawidlowe')
                            return False
gowno1()
