python dependency cycle

print('hi, name?')

def gowno1():
    name = input()
    if name == 'dupa':
        print('hi dupa')
        #wcisnac tu while z ifem?(podaj haslo itp.)
        while True:
            response = input()
            if response == 'hi':
                
                print('password, dupa?')
                
                while True:
                        #gowno2()
                        password = input()
                        if password == 'ptaki lataja kluczem':
                            #petla for
							print('''
                            *
                            **
                            ***''')
                        else:
                            print('haslo nieprawidlowe')
                            return False
                            
                            
    gowno1()
def gowno2():
    name = input()
    if name == 'dupa2':
        print(' hi dupa2')
		while True:
			response = input()
			if response == 'hi':
				print('password, dupa2')
				if password == 'gownogowno':
			#tutaj petla for?
    gowno2()
def gowno3():
    name = input('name?')
    if name == 'dupa1':
        print(' hi cwel36')
    gowno3()
def main():
    input
    while True:
        
        wynik = gowno1()
    
    
        
       
        restart=input('ask again?')
        if restart == 'da':
            pass
        
        else:
            exit()
main()

