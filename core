dupa = ['apples', 'bananas', 'tofu', 'cats']

def gowno(dupa):
    for x in dupa:
        return
    print(dupa)

y = ['apples', 'bananas', 'tofu', 'cats']
def function(x):
    x.insert(-1, ('and ' + x[-1]))
    del x[-1]
    numbers = len(x)
    spam = x[0]
    for i in range(1,numbers):
        spam = spam + ', ' + x[i]
    print(spam)

function(y)

