print('hi, name?')

def gowno1():
    name = input()
    if name == 'dupa':
        print('hi dupa')
        #wcisnac tu while z ifem?(podaj haslo itp.)
        while True:
            response = input()
            if response != 'hi':
				return False
			else:
                print('password, dupa?')
                
                while True:
                       gowno2()
                        password = input()
                        if password == 'ptaki lataja kluczem':
                            print('dada')
                        else:
                            print('haslo nieprawidlowe')
                            return False
                            
                            
    gowno1()
def gowno2():
    name = input()
    if name == 'dupa2':
        print(' hi dupa2')
    gowno2()
def gowno3():
    name = input('name?')
    if name == 'dupa1':
        print(' hi cwel36')
    gowno3()
def main():
    input
    while True:
        
        wynik = gowno1()
    
    
        
       
        restart=input('ask again?')
        if restart == 'da':
            pass
        
        else:
            exit()
main()
    

